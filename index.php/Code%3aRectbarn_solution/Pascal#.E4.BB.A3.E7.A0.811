<a name=".E4.BB.A3.E7.A0.811"></a><h2><span class="editsection">[<a href="/index.php?title=Code:Rectbarn_solution/Pascal&amp;action=edit&amp;section=1" title="编辑段落: 代码1">编辑</a>]</span> <span class="mw-headline">代码1</span></h2>
<pre class="pascal"><span style="color: #808080; font-style: italic;">{
ID:crboygi2
PROG:rectbarn
LANG:PASCAL
}</span>
<span style="color: #000000; font-weight: bold;">program</span> rectbarn;
  <span style="color: #993333;">type</span>
    ty=<span style="color: #993333;">record</span>
      x,y:<span style="color: #993333;">integer</span>;
     <span style="color: #b1b100;">end</span>;
  <span style="color: #b1b100;">var</span>
    l,r,h:<span style="color: #993333;">array</span><span style="color: #66cc66;">&#91;</span><span style="color: #cc66cc;">1</span>..<span style="color: #cc66cc;">3100</span><span style="color: #66cc66;">&#93;</span><span style="color: #b1b100;">of</span> <span style="color: #993333;">integer</span>;
<span style="color: #808080; font-style: italic;">{分别在i行时存储j列所能达到的左边界，右边界，和上边界h[j]表示到达j时的最大高度，此处的l,r,h会随i的变化而变化}</span>
    aa:<span style="color: #993333;">array</span><span style="color: #66cc66;">&#91;</span><span style="color: #cc66cc;">1</span>..<span style="color: #cc66cc;">31000</span><span style="color: #66cc66;">&#93;</span><span style="color: #b1b100;">of</span> ty;<span style="color: #808080; font-style: italic;">{存储不能利用的坐标}</span>
    n,m,k:<span style="color: #993333;">integer</span>;
    max:<span style="color: #993333;">longint</span>;<span style="color: #808080; font-style: italic;">{最后结果}</span>
  <span style="color: #000000; font-weight: bold;">procedure</span>  int;
    <span style="color: #b1b100;">var</span>
      i:<span style="color: #993333;">integer</span>;
    <span style="color: #b1b100;">begin</span>
      assign<span style="color: #66cc66;">&#40;</span>input,<span style="color: #ff0000;">'rectbarn.in'</span><span style="color: #66cc66;">&#41;</span>;
      reset<span style="color: #66cc66;">&#40;</span>input<span style="color: #66cc66;">&#41;</span>;
      <span style="">readln</span><span style="color: #66cc66;">&#40;</span>n,m,k<span style="color: #66cc66;">&#41;</span>;
      <span style="color: #b1b100;">for</span> i:=<span style="color: #cc66cc;">1</span> <span style="color: #b1b100;">to</span> k <span style="color: #b1b100;">do</span>
        <span style="">readln</span><span style="color: #66cc66;">&#40;</span>aa<span style="color: #66cc66;">&#91;</span>i<span style="color: #66cc66;">&#93;</span>.<span style="color: #202020;">x</span>,aa<span style="color: #66cc66;">&#91;</span>i<span style="color: #66cc66;">&#93;</span>.<span style="color: #202020;">y</span><span style="color: #66cc66;">&#41;</span>;
      close<span style="color: #66cc66;">&#40;</span>input<span style="color: #66cc66;">&#41;</span>;
    <span style="color: #b1b100;">end</span>;
  <span style="color: #000000; font-weight: bold;">procedure</span>  kuai<span style="color: #66cc66;">&#40;</span>s,t:<span style="color: #993333;">integer</span>;var ii:<span style="color: #993333;">integer</span><span style="color: #66cc66;">&#41;</span>;
    <span style="color: #b1b100;">var</span>
      temp:ty;
      i,j:<span style="color: #993333;">integer</span>;
    <span style="color: #b1b100;">begin</span>
      i:=s;j:=t;temp:=aa<span style="color: #66cc66;">&#91;</span>s<span style="color: #66cc66;">&#93;</span>;
      <span style="color: #b1b100;">while</span> i&lt;j <span style="color: #b1b100;">do</span>
        <span style="color: #b1b100;">begin</span>
          <span style="color: #b1b100;">while</span> <span style="color: #66cc66;">&#40;</span>i&lt;j<span style="color: #66cc66;">&#41;</span><span style="">and</span><span style="color: #66cc66;">&#40;</span><span style="color: #66cc66;">&#40;</span>aa<span style="color: #66cc66;">&#91;</span>j<span style="color: #66cc66;">&#93;</span>.<span style="color: #202020;">x</span>&gt;temp.<span style="color: #202020;">x</span><span style="color: #66cc66;">&#41;</span>or<span style="color: #66cc66;">&#40;</span><span style="color: #66cc66;">&#40;</span>aa
&nbsp;
<span style="color: #66cc66;">&#91;</span>j<span style="color: #66cc66;">&#93;</span>.<span style="color: #202020;">x</span>=temp.<span style="color: #202020;">x</span><span style="color: #66cc66;">&#41;</span><span style="">and</span><span style="color: #66cc66;">&#40;</span>aa<span style="color: #66cc66;">&#91;</span>j<span style="color: #66cc66;">&#93;</span>.<span style="color: #202020;">y</span>&gt;=temp.<span style="color: #202020;">y</span><span style="color: #66cc66;">&#41;</span><span style="color: #66cc66;">&#41;</span><span style="color: #66cc66;">&#41;</span> <span style="color: #b1b100;">do</span>
            dec<span style="color: #66cc66;">&#40;</span>j<span style="color: #66cc66;">&#41;</span>;
          aa<span style="color: #66cc66;">&#91;</span>i<span style="color: #66cc66;">&#93;</span>:=aa<span style="color: #66cc66;">&#91;</span>j<span style="color: #66cc66;">&#93;</span>;
          <span style="color: #b1b100;">while</span> <span style="color: #66cc66;">&#40;</span>i&lt;j<span style="color: #66cc66;">&#41;</span><span style="">and</span><span style="color: #66cc66;">&#40;</span><span style="color: #66cc66;">&#40;</span>aa<span style="color: #66cc66;">&#91;</span>i<span style="color: #66cc66;">&#93;</span>.<span style="color: #202020;">x</span>&lt;temp.<span style="color: #202020;">x</span><span style="color: #66cc66;">&#41;</span>or<span style="color: #66cc66;">&#40;</span><span style="color: #66cc66;">&#40;</span>aa
&nbsp;
<span style="color: #66cc66;">&#91;</span>i<span style="color: #66cc66;">&#93;</span>.<span style="color: #202020;">x</span>=temp.<span style="color: #202020;">x</span><span style="color: #66cc66;">&#41;</span><span style="">and</span><span style="color: #66cc66;">&#40;</span>aa<span style="color: #66cc66;">&#91;</span>i<span style="color: #66cc66;">&#93;</span>.<span style="color: #202020;">y</span>&lt;=temp.<span style="color: #202020;">y</span><span style="color: #66cc66;">&#41;</span><span style="color: #66cc66;">&#41;</span><span style="color: #66cc66;">&#41;</span> <span style="color: #b1b100;">do</span>
            inc<span style="color: #66cc66;">&#40;</span>i<span style="color: #66cc66;">&#41;</span>;
          aa<span style="color: #66cc66;">&#91;</span>j<span style="color: #66cc66;">&#93;</span>:=aa<span style="color: #66cc66;">&#91;</span>i<span style="color: #66cc66;">&#93;</span>;
        <span style="color: #b1b100;">end</span>;
      aa<span style="color: #66cc66;">&#91;</span>i<span style="color: #66cc66;">&#93;</span>:=temp;
      ii:=i;
    <span style="color: #b1b100;">end</span>;
  <span style="color: #000000; font-weight: bold;">procedure</span>  pai<span style="color: #66cc66;">&#40;</span>s,t:<span style="color: #993333;">integer</span><span style="color: #66cc66;">&#41;</span>;<span style="color: #808080; font-style: italic;">{快排}</span>
    <span style="color: #b1b100;">var</span>
      ii:<span style="color: #993333;">integer</span>;
    <span style="color: #b1b100;">begin</span>
      <span style="color: #b1b100;">if</span> s&lt;t <span style="color: #b1b100;">then</span>  <span style="color: #808080; font-style: italic;">{注意此处不要写成while s&lt;t do 要不然成死循环，我就犯了这样的低级错误}</span>
        <span style="color: #b1b100;">begin</span>
          kuai<span style="color: #66cc66;">&#40;</span>s,t,ii<span style="color: #66cc66;">&#41;</span>;
          pai<span style="color: #66cc66;">&#40;</span>s,ii<span style="color: #cc66cc;">-1</span><span style="color: #66cc66;">&#41;</span>;
          pai<span style="color: #66cc66;">&#40;</span>ii<span style="color: #cc66cc;">+1</span>,t<span style="color: #66cc66;">&#41;</span>;
        <span style="color: #b1b100;">end</span>;
    <span style="color: #b1b100;">end</span>;
  <span style="color: #000000; font-weight: bold;">procedure</span>  check<span style="color: #66cc66;">&#40;</span>s,t:<span style="color: #993333;">integer</span><span style="color: #66cc66;">&#41;</span>;<span style="color: #808080; font-style: italic;">{计算过程，注意一定要体味此过程。得高人指点}</span>
    <span style="color: #b1b100;">var</span>
      i:<span style="color: #993333;">integer</span>;
      temp:<span style="color: #993333;">longint</span>;
    <span style="color: #b1b100;">begin</span>
      <span style="color: #b1b100;">for</span> i:=s <span style="color: #b1b100;">to</span> t <span style="color: #b1b100;">do</span><span style="color: #808080; font-style: italic;">{枚举计算此中包含的矩阵}</span>
        <span style="color: #b1b100;">begin</span>
          inc<span style="color: #66cc66;">&#40;</span>h<span style="color: #66cc66;">&#91;</span>i<span style="color: #66cc66;">&#93;</span><span style="color: #66cc66;">&#41;</span>;
          <span style="color: #b1b100;">if</span> h<span style="color: #66cc66;">&#91;</span>i<span style="color: #66cc66;">&#93;</span>=<span style="color: #cc66cc;">1</span> <span style="color: #b1b100;">then</span><span style="color: #808080; font-style: italic;">{如果高度为1，则此矩阵的square为1*(t-s+1)}</span>
            <span style="color: #b1b100;">begin</span>
              l<span style="color: #66cc66;">&#91;</span>i<span style="color: #66cc66;">&#93;</span>:=s;
              r<span style="color: #66cc66;">&#91;</span>i<span style="color: #66cc66;">&#93;</span>:=t;
            <span style="color: #b1b100;">end</span>
          <span style="color: #b1b100;">else</span>
            <span style="color: #b1b100;">begin</span>
              <span style="color: #b1b100;">if</span> s&gt;l<span style="color: #66cc66;">&#91;</span>i<span style="color: #66cc66;">&#93;</span> <span style="color: #b1b100;">then</span> l<span style="color: #66cc66;">&#91;</span>i<span style="color: #66cc66;">&#93;</span>:=s;<span style="color: #808080; font-style: italic;">{检查更新左右边界}</span>
              <span style="color: #b1b100;">if</span> t&lt;r<span style="color: #66cc66;">&#91;</span>i<span style="color: #66cc66;">&#93;</span> <span style="color: #b1b100;">then</span> r<span style="color: #66cc66;">&#91;</span>i<span style="color: #66cc66;">&#93;</span>:=t;
            <span style="color: #b1b100;">end</span>;
          temp:=h<span style="color: #66cc66;">&#91;</span>i<span style="color: #66cc66;">&#93;</span>*<span style="color: #66cc66;">&#40;</span>r<span style="color: #66cc66;">&#91;</span>i<span style="color: #66cc66;">&#93;</span>-l<span style="color: #66cc66;">&#91;</span>i<span style="color: #66cc66;">&#93;</span><span style="color: #cc66cc;">+1</span><span style="color: #66cc66;">&#41;</span>;
          <span style="color: #b1b100;">if</span> temp&gt;max <span style="color: #b1b100;">then</span> max:=temp;
        <span style="color: #b1b100;">end</span>;
     <span style="color: #b1b100;">end</span>;
  <span style="color: #000000; font-weight: bold;">procedure</span>  main;<span style="color: #808080; font-style: italic;">{注意体味此过程}</span>
    <span style="color: #b1b100;">var</span>
      i,j,newl,p:<span style="color: #993333;">integer</span>;
    <span style="color: #b1b100;">begin</span>
      max:=<span style="color: #cc66cc;">0</span>;p:=<span style="color: #cc66cc;">1</span>;
      fillchar<span style="color: #66cc66;">&#40;</span>h,sizeof<span style="color: #66cc66;">&#40;</span>h<span style="color: #66cc66;">&#41;</span>,<span style="color: #cc66cc;">0</span><span style="color: #66cc66;">&#41;</span>;<span style="color: #808080; font-style: italic;">{高度赋初始值}</span>
      <span style="color: #808080; font-style: italic;">{for i:=1 to m do
        begin
          l[i]:=1;
          r[i]:=m;
        end; }</span>
      <span style="color: #b1b100;">for</span> i:=<span style="color: #cc66cc;">1</span> <span style="color: #b1b100;">to</span> n <span style="color: #b1b100;">do</span>
        <span style="color: #b1b100;">begin</span>
          newl:=<span style="color: #cc66cc;">1</span>;    <span style="color: #808080; font-style: italic;">{左边界赋初始值}</span>
          <span style="color: #b1b100;">for</span> j:=<span style="color: #cc66cc;">1</span> <span style="color: #b1b100;">to</span> m <span style="color: #b1b100;">do</span>
            <span style="color: #b1b100;">if</span> <span style="color: #66cc66;">&#40;</span>i=aa<span style="color: #66cc66;">&#91;</span>p<span style="color: #66cc66;">&#93;</span>.<span style="color: #202020;">x</span><span style="color: #66cc66;">&#41;</span><span style="">and</span><span style="color: #66cc66;">&#40;</span>j=aa<span style="color: #66cc66;">&#91;</span>p<span style="color: #66cc66;">&#93;</span>.<span style="color: #202020;">y</span><span style="color: #66cc66;">&#41;</span> <span style="color: #b1b100;">then</span>
              <span style="color: #b1b100;">begin</span>
                check<span style="color: #66cc66;">&#40;</span>newl,j<span style="color: #cc66cc;">-1</span><span style="color: #66cc66;">&#41;</span>;
                newl:=j<span style="color: #cc66cc;">+1</span>;<span style="color: #808080; font-style: italic;">{更新左边界}</span>
                h<span style="color: #66cc66;">&#91;</span>j<span style="color: #66cc66;">&#93;</span>:=<span style="color: #cc66cc;">0</span>;<span style="color: #808080; font-style: italic;">{此时，j列的高度变为0}</span>
                <span style="color: #b1b100;">while</span> <span style="color: #66cc66;">&#40;</span>aa<span style="color: #66cc66;">&#91;</span>p<span style="color: #66cc66;">&#93;</span>.<span style="color: #202020;">y</span>=j<span style="color: #66cc66;">&#41;</span><span style="">and</span><span style="color: #66cc66;">&#40;</span>aa<span style="color: #66cc66;">&#91;</span>p<span style="color: #66cc66;">&#93;</span>.<span style="color: #202020;">x</span>=i<span style="color: #66cc66;">&#41;</span> <span style="color: #b1b100;">do</span>
                  inc<span style="color: #66cc66;">&#40;</span>p<span style="color: #66cc66;">&#41;</span>;
              <span style="color: #b1b100;">end</span>;
          check<span style="color: #66cc66;">&#40;</span>newl,m<span style="color: #66cc66;">&#41;</span>;<span style="color: #808080; font-style: italic;">{计算i行时，最右边的不能用的点到右边界}</span>
        <span style="color: #b1b100;">end</span>;
    <span style="color: #b1b100;">end</span>;
  <span style="color: #000000; font-weight: bold;">procedure</span>  out1;
    <span style="color: #b1b100;">begin</span>
      assign<span style="color: #66cc66;">&#40;</span>output,<span style="color: #ff0000;">'rectbarn.out'</span><span style="color: #66cc66;">&#41;</span>;
      rewrite<span style="color: #66cc66;">&#40;</span>output<span style="color: #66cc66;">&#41;</span>;
     <span style="color: #808080; font-style: italic;">{ for i:=1 to k do{检查快排是否正确}</span>
        <span style="">write</span><span style="color: #66cc66;">&#40;</span>aa<span style="color: #66cc66;">&#91;</span>i<span style="color: #66cc66;">&#93;</span>.<span style="color: #202020;">x</span>:<span style="color: #cc66cc;">4</span><span style="color: #66cc66;">&#41;</span>;
      <span style="">writeln</span>;
      <span style="color: #b1b100;">for</span> i:=<span style="color: #cc66cc;">1</span> <span style="color: #b1b100;">to</span> k <span style="color: #b1b100;">do</span>
        <span style="">write</span><span style="color: #66cc66;">&#40;</span>aa<span style="color: #66cc66;">&#91;</span>i<span style="color: #66cc66;">&#93;</span>.<span style="color: #202020;">y</span>:<span style="color: #cc66cc;">4</span><span style="color: #66cc66;">&#41;</span>;
      <span style="">writeln</span>;<span style="color: #66cc66;">&#125;</span>
      <span style="">writeln</span><span style="color: #66cc66;">&#40;</span>max<span style="color: #66cc66;">&#41;</span>;
      close<span style="color: #66cc66;">&#40;</span>output<span style="color: #66cc66;">&#41;</span>;
